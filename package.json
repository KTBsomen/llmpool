    "axios": "^1.6.0",
    "gpt-tokenizer": "^2.1.2"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "eslint": "^8.55.0",
    "prettier": "^3.1.0",
    "@types/node": "^20.10.0"
  },
  "engines": {
    "node": ">=16.0.0"
